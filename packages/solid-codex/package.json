{
  "name": "solid-codex",
  "scripts": {
    "dev": "solid-start dev",
    "build": "solid-start build",
    "build:cjs": "tsc --project tsconfig.json --outDir ./dist/cjs --module CommonJS",
    "build:mjs": "tsc --project tsconfig.json --outDir ./dist/mjs --module ESNext",
    "build:svg": "svgr --template ./node_modules/dolmen/scripts/svg-template.cjs --typescript iconsrc/ -d src/icons/",
    "clean": "rimraf ./dist",
    "start": "solid-start start",
    "test:compile": "tsc --noEmit --project ./tsconfig.json"
  },
  "type": "module",
  "source": "./src/index.ts",
  "module": "./src/index.ts",
  "devDependencies": {
    "solid-start-node": "^0.1.6",
    "typescript": "^4.8.3",
    "vite": "^3.1.0"
  },
  "dependencies": {
    "@solidjs/meta": "^0.28.0",
    "@solidjs/router": "^0.5.0",
    "@vanilla-extract/css": "^1.9.1",
    "dolmen": "^0.0.1",
    "solid-codex-api": "^0.0.1",
    "solid-js": "^1.5.7",
    "solid-mdx": "^0.0.6",
    "solid-start": "^0.1.6",
    "undici": "^5.10.0"
  },
  "engines": {
    "node": ">=16"
  }
}
